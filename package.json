{
  "name": "vs-code-file-status",
  "displayName": "File Status Stepper",
  "description": "Jump to the next information/warning/error without scrolling",
  "publisher": "denniseriksson93",
  "icon": "images/example.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/denniseriksson93/vs-code-file-status"
  },
  "license": "MIT",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-code-file-status.file-information",
        "title": "Show information",
        "category": "Status",
        "icon": "$(info)"
      },
      {
        "command": "vs-code-file-status.file-warning",
        "title": "Show warning",
        "category": "Status",
        "icon": "$(warning)"
      },
      {
        "command": "vs-code-file-status.file-error",
        "title": "Show error",
        "category": "Status",
        "icon": "$(error)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "vs-code-file-status.file-information",
          "group": "navigation",
          "when": "show-information"
        },
        {
          "command": "vs-code-file-status.file-warning",
          "group": "navigation",
          "when": "show-warning"
        },
        {
          "command": "vs-code-file-status.file-error",
          "group": "navigation",
          "when": "show-error"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
